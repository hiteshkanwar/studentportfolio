<% @student_requests.each do |request| %>

	<%= request.student.user.firstname %>
	<%= request.student.user.lastname %>
	<% if request.student.student_schools.first.status == true %>
		<%=link_to "Approved", "#" %>
	<% else %>
		<%=link_to "Approve", home_approve_reject_path(:student_id=>request.student.id), remote: true%>
	<% end %>
    <%=link_to "Reject",  home_approve_reject_path(:reject=>true,:student_id=>request.student.id),remote: true%><br/>
<% end %>